  "description": "Node boiler is a library that automatically writes boiler plate code which otherwise does not come out of the box with node.js. This tool comes in handy while make REST APIs.",
  "main": "index.js",
  "keywords": [
    "nodejs",
    "javascript",
    "code generation",
    "boiler plate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayankshah1607/node-boiler.git"
  },
  "scripts": {
    "test": "mocha",
    "start": "node index.js"
  },
  "engines": {
    "node": ">=10.0.0 <11.0.0",
    "npm": ">=6.0.0 <7.0.0 "
  },
  "author": "Mayank Shah",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.4",
    "js-yaml": "^3.13.1"
  },
  "bin": {
    "nodeboil": "./bin/nodeboil.js"
  },
  "preferGlobal": true,
  "devDependencies": {
    "fs-extra": "^8.0.0",
    "mocha": "^10.2.0",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0"
  },
  "files": [
    "/assets",
    "/lib",
    "/bin",
    "/package.json",
    "README.md",
    "LICENSE"
